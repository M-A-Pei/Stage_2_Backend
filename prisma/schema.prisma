generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Posts {
  id       Int     @id @default(autoincrement())
  title    String
  body     String
}

model Users {
  id          Int     @id @default(autoincrement())
  username    String
  password    String
  profilePic  String?
}
